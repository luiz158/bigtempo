#!/bin/bash


CURRDIR="$(pwd)"
PROJECT_LOCATION="$( cd "$( dirname "${BASH_SOURCE[0]}" )" && cd .. && pwd )"
export PYTHONPATH="${PROJECT_LOCATION}/stockExperiments"

cd "${PROJECT_LOCATION}"
echo
echo "RUNNING"
echo "--------------------------------------------------------------------------------"
python stockExperiments/experiments.py
echo

find . -type f -name "*.pyc" -exec rm -f {} \;
cd "${CURRDIR}"
