#!/bin/bash


CURRDIR="$(pwd)"
PROJECT_LOCATION="$( cd "$( dirname "${BASH_SOURCE[0]}" )" && cd .. && pwd )"
export PYTHONPATH="${PROJECT_LOCATION}/stockExperiments"

cd $PROJECT_LOCATION
echo
echo "SAVING REQUIREMENTS"
echo "--------------------------------------------------------------------------------"
pip freeze > requirements.txt
echo
cd "${CURRDIR}"
